main() -> int {
	// 1D list
	[int] l1 = [5];
	int elem = l1[0];
	print_int(elem);
	print_string("\n");

	l1[0] = 100;
	print_int(l1[0]);
	print_string("\n");

	// 2D list
	[[int]] l2 = [[0, 1, 2], [3, 4, 5]];

	int i = 0;
	int j = 0;

	for (i = 0; i < 2; i = i + 1) {
		for (j = 0; j < 3; j = j + 1) {
			l2[i][j] = l2[i][j] * 100;
		}
	}

	for (i = 0; i < 2; i = i + 1) {
		for (j = 0; j < 3; j = j + 1) {
			print_int(l2[i][j]);
			print_string(" ");
		}
	}

	return 0;
}
